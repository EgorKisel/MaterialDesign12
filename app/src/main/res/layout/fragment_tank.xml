<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/idid"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/my_tank"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/tank"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.708"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/my_tank2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/tank"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.22"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/my_tank"
        app:layout_constraintVertical_bias="0.484" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/my_tank3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/tank"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/my_tank2" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/my_tank4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/tank"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.868"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/my_tank3"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/my_tank5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/tank"
        app:layout_constraintBottom_toTopOf="@id/my_tank"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>